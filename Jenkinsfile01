pipeline {
  
  agent any

  environment {
    CONFIG = 'Release'
    PLATFORM = 'x86'
  }
  
  stages {
    stage('Build') {
      steps {
        bat "\"${MSBUILD}\" /p:Configuration=${env.CONFIG}; /p:AppxBundlePlatforms=${env.PLATFORM}  /p:AppxBundle=Never /p:UapAppxPackageBuildMode=Sideloading  /p:AppxPackageSigningEnabled=false"
        
      }
    }
  }
}